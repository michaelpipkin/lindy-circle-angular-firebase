{
  "name": "lindy-circle-angular-firebase",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration production",
    "build-fx": "cd functions && npm run build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "firebase": "cd functions && npm run build && cd .. && npx kill-port 4000, 9099, 5001, 8080, 5000, 9199 && firebase emulators:start --import seed-data",
    "export": "firebase emulators:export seed-data"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "*",
    "@angular/cdk": "*",
    "@angular/common": "*",
    "@angular/compiler": "*",
    "@angular/core": "*",
    "@angular/fire": "*",
    "@angular/forms": "*",
    "@angular/material": "*",
    "@angular/platform-browser": "*",
    "@angular/platform-browser-dynamic": "*",
    "@angular/router": "*",
    "bootstrap": "*",
    "firebase": "*",
    "firebase-functions": "*",
    "firebaseui": "*",
    "rxjs": "*",
    "tslib": "*",
    "zone.js": "*"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "17.2.2",
    "@angular/cli": "17.2.2",
    "@angular/compiler-cli": "*",
    "@types/jasmine": "*",
    "jasmine-core": "*",
    "karma": "*",
    "karma-chrome-launcher": "*",
    "karma-coverage": "*",
    "karma-jasmine": "*",
